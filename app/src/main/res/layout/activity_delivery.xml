<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bck"
    tools:context=".delivery">

    <TextView
        android:id="@+id/t34"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="@string/textview34"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        android:layout_alignParentLeft="true" />

    <Button
        android:id="@+id/b13"
        android:layout_width="131dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="33dp"
        android:layout_marginTop="134dp"
        android:background="@android:color/holo_red_dark"
        android:text="@string/button13"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        android:layout_alignParentRight="true"
        android:layout_marginRight="33dp" />

    <TextView
        android:id="@+id/t35"
        android:layout_width="276dp"
        android:layout_height="44dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="40dp"
        android:layout_marginTop="56dp"
        android:text="@string/textview35"
        android:textAllCaps="true"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        android:layout_alignParentRight="true"
        android:layout_marginRight="40dp" />

    <TextView
        android:id="@+id/t36"
        android:layout_width="84dp"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="101dp"
        android:layout_marginTop="137dp"
        android:text="@string/textview36"
        android:textSize="18sp"
        android:textStyle="bold|italic"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="101dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="43dp"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:background="@android:color/black"
        app:srcCompat="@android:drawable/ic_menu_slideshow"
        android:layout_alignParentRight="true" />

    <TextView
        android:id="@+id/t37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="227dp"
        android:text="@string/textview37"
        android:textSize="18sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="205dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/t35"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="211dp"
        android:text="@string/textview38"
        android:textSize="25sp"
        android:textStyle="italic"
        android:layout_alignRight="@+id/t35" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="58dp"
        android:layout_height="47dp"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView8"
        android:layout_marginStart="58dp"
        app:srcCompat="@drawable/medal"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="58dp" />
</RelativeLayout>